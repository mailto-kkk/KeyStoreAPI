{
    "name": "vault_api",
    "version": "1.0.0",
    "description": "VAULT API",
    "author": "Kumaresan",
    "main": "index.js",
    "scripts": {
        "unitTest": "grunt unitTest",
        "coverage": "grunt coverage",
        "start": "node index.js",
        "startXLSACCOUNTAPIServer": "forever start index.js",
        "stopXLSACCOUNTAPIServer": "forever stop index.js",
        "restartXLSACCOUNTAPIServer": "forever restart index.js",
        "xtest": "./node_modules/.bin/mocha --recursive -R xunit test/ > test-reports.xml",
        "coverageRpt": "./node_modules/.bin/istanbul report cobertura --root coverage --dir coverage",
        "jutest": "./node_modules/.bin/mocha \"test/unit/\" --reporter mocha-junit-reporter --timeout 30000",
        "integTest": "./node_modules/.bin/mocha test --timeout 15000",
        "integrationTest": "node test/integration/newmanTestSuite.js"
    },
    "engines": {
        "node": "10.15.0"
    },
    "dependencies": {
        "express": "4.16.4",
        "kraken-js": "2.2.0",
        "nconf": "0.10.0",
        "log4js": "3.0.6",
        "forever": "0.15.3",
        "hashmap": "2.3.0",
        "stack-trace": "0.0.10",
        "mocha-junit-reporter": "1.18.0",
        "q": "1.5.1",
        "node-gyp": "3.8.0",
        "http-status": "1.3.1",
        "jsonschema": "1.2.4",
        "async": "2.6.1",
        "moment": "2.23.0",
        "mysql": "2.13.0",
        "object-unset": "1.0.1",
        "newman": "4.3.1",
        "lodash": "4.17.11",
        "validator": "10.10.0"
    },
    "devDependencies": {
        "babel-cli": "6.26.0",
